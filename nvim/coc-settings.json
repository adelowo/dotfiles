{ "diagnostic.warningSign": "⚠️",
  "diagnostic.errorSign": "❌",
  "diagnostic.hintSign": "⚠️",
  "diagnostic.infoSign": "⚠️",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ]
    },
    "solidity": {
      "command": "/path/to/solc",
      "args": [
        "--lsp"
      ],
      "trace.server": "verbose",
      "rootPatterns": [
        ".git/"
      ],
      "filetypes": [
        "solidity"
      ]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "html",
    "scss",
    "js",
    "tsx",
    "jsx",
    "php"
  ],
  "tsserver.npm": "/usr/local/bin/npm"
}
